            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Button>
        <Button fx:id="btnDelete" layoutX="238.0" layoutY="304.0" mnemonicParsing="false" onMouseClicked="#actionUserDelete" prefHeight="40.0" prefWidth="150.0" text="DELETE">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Button>
        <ComboBox fx:id="cbUserName" layoutX="269.0" layoutY="51.0" onAction="#writeInfos" prefHeight="26.0" prefWidth="242.0" promptText="Chọn tên người dùng muốn xoá" />
        <Label layoutX="145.0" layoutY="54.0" text="User">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
