<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.librarymanagement.UserController.UserDeleteController">
    <children>
        <Label layoutX="145.0" layoutY="90.0" text="User Email">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Label>
        <Label layoutX="145.0" layoutY="125.0" text="User FullName">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Label>
        <Label layoutX="145.0" layoutY="160.0" text="User Address">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Label>
        <Label layoutX="146.0" layoutY="193.0" prefHeight="20.0" prefWidth="118.0" text="User Phone">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Label>
        <TextField fx:id="txtUserEmail" layoutX="269.0" layoutY="87.0" prefHeight="26.0" prefWidth="242.0" />
        <TextField fx:id="txtUserName" layoutX="269.0" layoutY="123.0" prefHeight="26.0" prefWidth="242.0" />
        <TextField fx:id="txtUserAddress" layoutX="269.0" layoutY="157.0" prefHeight="26.0" prefWidth="242.0" />
